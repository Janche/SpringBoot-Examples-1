spring:
  application:
    name: redis
  redis:  # redis连接哨兵/集群模式参考文章 https://blog.csdn.net/qq_39669058/article/details/89158145
    database: 1
    jedis:
      pool:
        block-when-exhausted: true
        max-active: 200
        max-idle: 100
        max-redirects: 3
        max-wait: 5000ms
        min-idle: 8
        test-on-borrow: true
        test-on-return: false
    password: 123456
    sentinel:
      master: mymaster
      nodes:
        - 10.197.236.154:26379
        - 10.197.236.169:26379
        - 10.197.236.184:26379
    timeout: 5000ms
server:
  port: 10005
